{
  "name": "frontron-react",
  "version": "0.0.1",
  "type": "module",
  "author": "안동민",
  "main": "src/frontron/main.cjs",
  "build": {
    "appId": "Frontron React",
    "mac": {
      "icon": "public/icon.png"
